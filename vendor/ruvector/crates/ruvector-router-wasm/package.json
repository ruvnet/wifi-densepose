{
  "name": "@ruvector/router-wasm",
  "version": "0.1.0",
  "description": "WebAssembly bindings for ruvector-router - Semantic router with HNSW vector search for browsers",
  "main": "pkg/ruvector_router_wasm.js",
  "types": "pkg/ruvector_router_wasm.d.ts",
  "files": [
    "pkg/"
  ],
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg",
    "build:node": "wasm-pack build --target nodejs --out-dir pkg-node",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg-bundler",
    "test": "wasm-pack test --headless --firefox"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/ruvector"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "semantic-router",
    "hnsw",
    "vector-search",
    "ai-routing",
    "browser"
  ],
  "author": "rUv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ruvnet/ruvector/issues"
  },
  "homepage": "https://github.com/ruvnet/ruvector"
}
