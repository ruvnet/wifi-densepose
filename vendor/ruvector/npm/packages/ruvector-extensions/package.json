    "cohere",
    "graph-visualization",
    "neo4j",
    "temporal-tracking",
    "persistence"
  ],
  "author": "ruv.io Team <info@ruv.io> (https://ruv.io)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ruvnet/ruvector.git",
    "directory": "npm/packages/ruvector-extensions"
  },
  "dependencies": {
    "ruvector": "^0.1.20",
    "@anthropic-ai/sdk": "^0.24.0",
    "express": "^4.18.2",
    "ws": "^8.16.0"
  },
  "peerDependencies": {
    "openai": "^4.0.0",
    "cohere-ai": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    },
    "cohere-ai": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
